{
    "name": "kodash",
    "version": "2.0.0",
    "moduleType": [
        "amd"
    ],
    "main": [
        "javascripts/kodash.js",
        "javascripts/kodash.min.js"
    ],
    "keywords": [
        "knockout",
        "lodash",
        "functional"
    ],
    "authors": [
        "Sam Armstrong"
    ],
    "ignore": [
        "source",
        "spec",
        ".bowerrc",
        ".gitignore",
        ".gitattributes",
        ".jshintignore",
        ".jshintrc",
        "bower.json",
        "gruntfile.js",
        "package.json",
        "README.md"
    ],
    "dependencies": {
        "knockout": ">=2.3.0",
        "lodash": ">=2.4.0"
    },
    "exportsOverride": {
        "lodash": {
            "": "**/*.js"
        }
    }
}